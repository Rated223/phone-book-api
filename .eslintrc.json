{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "node/no-path-concat": "off",
    "node/no-unsupported-features/es-syntax": "off",
    "node/no-missing-import": "off"
  }
}
